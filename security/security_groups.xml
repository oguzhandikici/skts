<odoo>

    <record model="ir.module.category" id="module_category_skts">
        <field name="name">SKTS Modülü</field>
    </record>

    <record id="group_skts_user" model="res.groups">
        <field name="name">SKTS Kullanıcısı</field>
        <field name="users" eval="[(4, ref('base.user_demo'))]"/>
        <field name="category_id" ref="module_category_skts"/>
    </record>

    <record id="group_skts_manager" model="res.groups">
        <field name="name">SKTS Yöneticisi</field>
        <field name="category_id" ref="module_category_skts"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>

</odoo>