<odoo>
    <record model="ir.ui.view" id="skts_school_term_view_tree">
        <field name="name">skts.school.term.tree</field>
        <field name="model">skts.school.term</field>
        <field name="arch" type="xml">
            <tree>
                <field name="school_id"/>
                <field name="name"/>
                <field name="date_start"/>
                <field name="date_end"/>
                <field name="open_to_register"/>
            </tree>
        </field>
    </record>

    <record model="ir.ui.view" id="skts_registration_school_term_view_tree">
        <field name="name">skts.school.term.tree</field>
        <field name="model">skts.school.term</field>
        <field name="priority">100</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="date_start"/>
                <field name="date_end"/>
            </tree>
        </field>
    </record>


    <record model="ir.actions.act_window" id="skts_school_term_action">
        <field name="name">School Terms</field>
        <field name="res_model">skts.school.term</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="skts_school_term_menu"
              name="School Terms"
              action="skts_school_term_action"
              sequence="71"
              groups="base.group_no_one"
              parent="skts_configuration_menu"/>

</odoo>
